<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane id="AnchorPane" fx:id="loginAnchorPane" prefHeight="250.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ServisChoose">
   <Pane prefHeight="250.0" prefWidth="300.0" style="-fx-background-color: #FFFFFF;">
      <VBox alignment="CENTER_LEFT" layoutX="41.0" layoutY="45.0" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="191.0" prefWidth="219.0" spacing="10.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
         </padding>
         <ComboBox fx:id="servisChooseComboBox" prefHeight="25.0" prefWidth="215.0" promptText="%chooseService"/>
         <HBox id="HBox" alignment="CENTER" spacing="5.0"/>
         <HBox id="HBox" alignment="CENTER" spacing="50.0">
            <Button fx:id="servisChooseButton" mnemonicParsing="false" onAction="#servisChooseButtonAction"
                    prefHeight="30.0" prefWidth="100.0"
                    style="-fx-background-radius: 20; -fx-border-radius: 20; -fx-background-color: #3b3b3b;"
                    text="%choose" textAlignment="CENTER" textFill="WHITE"/>
         </HBox>
      </VBox>
   </Pane>
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#313131" height="392.0" layoutX="-67.0" layoutY="-165.0"
              rotate="48.0" stroke="TRANSPARENT" strokeType="INSIDE" width="146.0"/>
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2c2c2c" height="392.0" layoutX="-59.0" layoutY="-224.0"
              rotate="48.0" stroke="TRANSPARENT" strokeType="INSIDE" width="146.0"/>
   <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="-4.0" layoutY="2.0" rotate="90.0">
      <Image url="@sipka.png"/>
   </ImageView>
   <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" opacity="0.0" prefHeight="30.0"
           prefWidth="24.0" textAlignment="CENTER" textFill="WHITE"/>
</AnchorPane>
